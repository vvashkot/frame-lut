{
  "name": "@archon/lut-action",
  "version": "1.0.0",
  "type": "module",
  "description": "Frame.io v4 Custom Action service for applying LUTs to video assets",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch --clear-screen=false src/server.ts",
    "build": "tsc -p tsconfig.json",
    "postbuild": "npx tsx scripts/importLUTs.ts luts/ || echo 'LUT import failed but continuing'",
    "build:railway": "npm run build && npx tsx scripts/importLUTs.ts luts/",
    "start": "node dist/server.js",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "vitest run -c vitest.integration.config.ts",
    "test:coverage": "vitest run --coverage",
    "seed:luts": "tsx scripts/seedLUTs.ts",
    "import:luts": "tsx scripts/importLUTs.ts",
    "frameio:info": "tsx scripts/getFrameioInfo.ts",
    "frameio:refresh": "tsx scripts/refreshToken.ts",
    "register:action": "tsx scripts/registerCustomAction.ts",
    "setup:webhook": "tsx scripts/setupWebhook.ts",
    "test:lut": "tsx scripts/testLUTProcessing.ts",
    "worker": "tsx src/worker.ts",
    "worker:prod": "node dist/worker.js"
  },
  "dependencies": {
    "@types/express": "^4.17.21",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "form-data": "^4.0.0",
    "helmet": "^7.1.0",
    "jose": "^5.2.4",
    "multer": "^1.4.5-lts.1",
    "pino": "^9.0.0",
    "pino-http": "^10.0.0",
    "pino-pretty": "^11.0.0",
    "sharp": "^0.33.3",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.7",
    "@types/sharp": "^0.31.1",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "@vitest/coverage-v8": "^1.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.9",
    "vitest": "^1.5.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}